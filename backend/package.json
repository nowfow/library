{
  "name": "music-library-backend",
  "version": "1.0.0",
  "description": "Backend API для музыкальной библиотеки нот валторны",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "node test/test.js",
    "db:init": "node scripts/db-manager.js init",
    "db:reset": "node scripts/db-manager.js reset",
    "db:status": "node scripts/db-manager.js status",
    "db:backup": "node scripts/db-manager.js backup",
    "db:populate": "node populate-db.js",
    "create-admin": "node scripts/db-manager.js create-admin",
    "import-terms": "node scripts/import-terms.js",
    "generate-thumbnails": "node -e \"import('./src/utils/pdf-thumbnails.js').then(m => m.generateThumbnailsForAllWorks())\""
  },
  "keywords": ["music", "library", "horn", "notes", "api"],
  "author": "Music Library Project",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "express-session": "^1.17.3",
    "pdf-lib": "^1.17.1",
    "canvas": "^2.11.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}